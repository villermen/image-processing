{
    "name": "villermen/image-processing",
    "description": "Library for processing and storing images from external sources.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "villermen/data-handling": "^1.0",
        "ext-gd": "*",
        "ext-exif": "*",
        "brianmcdo/image-palette": "^2.0",
        "symfony/process": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "gifsicle binary": "If you want to resize GIFs while keeping their animations."
    },
    "autoload": {
        "psr-4": {
            "Villermen\\ImageProcessing\\": "src/"
        }
    }
}
